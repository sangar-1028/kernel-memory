{
  "SKMemory": {
    // =====================
    // ====== Content ======
    // =====================
    "ContentStorage": {
      "Type": "FileSystem",
      "FileSystem": {
        // local folder, e.g. under ./bin/Debug
        "Directory": "./test"
      },
    },
    // ===========================
    // ====== Orchestration ======
    // ===========================
    "Orchestration": {
      "Type": "InProcess"
    },
    // ===============================
    // ====== Pipeline Handlers ======
    // ===============================
    "Handlers": {
      "gen_embeddings": {
        "EmbeddingGeneration": {
          "ActiveGenerators": [
            // "AzureAda"
            // "OpenAIAda"
          ],
          "GeneratorsConfig": {
            "AzureAda": {
              "Type": "AzureOpenAI",
              "Endpoint": "https://<...>.openai.azure.com/",
              "APIKey": "",
              "Deployment": "",
            },
            "OpenAIAda": {
              "Type": "OpenAI",
              "Model": "text-embedding-ada-002",
              "APIKey": "",
            },
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
